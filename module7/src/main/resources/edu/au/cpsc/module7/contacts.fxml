<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="contacts-anchorpane" prefHeight="600.0" prefWidth="800.0" stylesheets="@theme.css" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.au.cpsc.module7.StudyController">
   <children>
      <VBox layoutX="337.0" layoutY="506.0" prefHeight="126.0" prefWidth="160.0">
         <children>
            <Button id="contacts-save-button" mnemonicParsing="false" onAction="#saveContacts" prefHeight="53.0" prefWidth="155.0" text="Save Contact" />
            <Button id="contacts-delete-button" mnemonicParsing="false" onAction="#deleteContacts" prefHeight="53.0" prefWidth="155.0" text="Delete Contact">
               <VBox.margin>
                  <Insets bottom="10.0" top="20.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
      <TextArea fx:id="contactsTextArea" layoutX="49.0" layoutY="90.0" prefHeight="99.0" prefWidth="533.0" />
      <ListView fx:id="contactsListView" layoutX="50.0" layoutY="255.0" prefHeight="238.0" prefWidth="700.0" />
      <Label id="contacts-top-label" layoutX="30.0" layoutY="38.0" prefHeight="48.0" prefWidth="742.0" text="New Contact: ***Seperate Name and Contact with comma***">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label id="contacts-bottom-label" layoutX="41.0" layoutY="203.0" prefHeight="41.0" prefWidth="125.0" text="Contacts:">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
